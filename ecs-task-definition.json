{
  "family": "PLACEHOLDER_FAMILY",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "PLACEHOLDER_CONTAINER_NAME",
      "image": "PLACEHOLDER_IMAGE",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3030,
          "hostPort": 3030,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::PLACEHOLDER_ACCOUNT_ID:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::PLACEHOLDER_ACCOUNT_ID:role/ecsTaskExecutionRole"
}
